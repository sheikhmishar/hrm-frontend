{
  "name": "skylane-hrm-frontend",
  "version": "1.0.0",
  "homepage": "/",
  "private": true,
  "type": "module",
  "dependencies": {
    "@tanstack/react-query": "^4.33.0",
    "chart.js": "^4.4.0",
    "papaparse": "^5.4.1",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.10.1",
    "react-router-dom": "^6.14.2",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@types/chart.js": "^2.9.38",
    "@types/node": "^16.18.70",
    "@types/papaparse": "^5.3.14",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "@types/react-icons": "^3.0.0",
    "@types/react-query": "^1.2.9",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.1.1",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "typescript": "^4.9.5",
    "vite": "^4.5.0",
    "vite-plugin-svgr": "^4.1.0",
    "vite-tsconfig-paths": "^4.2.1"
  },
  "scripts": {
    "dev": "vite --host",
    "build": "tsc && vite build",
    "export": "npm run build && npm run run:purgecss:global && npm run run:fontmin:global",
    "link:purgecss-fontmin:global": "npm link purgecss fontmin @types/fontmin",
    "run:purgecss:global": "node ./src/libs/purgecss/index.cjs",
    "run:fontmin:global": "node ./src/libs/fontmin/index.cjs",
    "typecheck": "tsc -p ./tsconfig.json --watch",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">1%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
